    {{#each model as |category|}}
        <div class="catalog-title-banner" id="{{category.headerBgId}}">
            <a href="#" {{action "displayProducts" category.id}}>
                <h1>{{category.name}}</h1>
            </a>
        </div>
        <div class="catalog-categories-body" id="{{category.backgroundId}}">
            <div class="row">
                <div class="container">
                    {{#if category.displayProducts}}
                        {{#each category.products as |product|}}
                            {{#if isExtended}}
                                <div class="expanded-product-wrapper">
                                    <div class="col-md-12">
                                        <div {{action "contract" on="keyPress"}}>
                                            <i class="glyphicon glyphicon-envelope"></i>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="ind-product-wrapper">
                                                <img src="{{product.productImg}}"><br/>
                                                <img class="thumbnailImage" src="{{product.productImg}}">
                                                <img class="thumbnailImage" src="{{product.labelImg}}">
                                            </div>
                                        </div>
                                        <div class="col-md-8">
                                            <div class="product-info-wrapper">
                                                <h1>{{product.engName}}</h1>
                                                <h4>{{product.spaName}} - {{product.quantity}}</h4>
                                                <p>{{product.description}}</p>
                                                <button class="btn btn-default"><a href="#">Get a Quote</a></button> 
                                                <button class="btn btn-default"><a href="#">Download Catalog</a></button>
                                            </div>
                                        </div>
                                    </div>
                                </div>    
                            {{else}}
                                <div class="col-md-4">
                                    <div  class="ind-product-wrapper">
                                        <div {{action "product.expand" on="keyPress"}}> 
                                            <img src="{{product.productImg}}" >
                                            <h5>{{product.engName}}</h5>
                                            <p>{{product.spaName}}<br/>{{product.quantity}}</p>
                                        </div>
                                    </div>
                                </div>
                            {{/if}}
                        {{/each}}
                    {{/if}}
                </div>
            </div>
        </div>                  
    {{/each}}

